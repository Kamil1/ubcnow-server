# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /api/v1/blip                controllers.Blips.list()
POST    /api/v1/blip                controllers.Blips.create()
GET     /api/v1/blip/:id            controllers.Blips.get(id: Long)
